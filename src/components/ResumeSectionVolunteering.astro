---
import dayjs from 'dayjs'

const { items } = Astro.props;
---

{items.map((p) => (
  <div class="mb-8">
    <h3 class="text-xl lg:text-2xl text-cyan-500 font-bold mb-2">{p.organization}</h3>
    <p>{p.position}</p>
    <p>{dayjs(p.startDate).format('YYYY')} - {p.endDate ? dayjs(p.endDate).format('YYYY') : 'Now'}</p>
  </div>
))}