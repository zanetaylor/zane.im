---
import MainLayout from '$/layouts/Main.astro'

import Footer from '$/components/Footer.astro'

import ResumeSectionTitle from '$/components/ResumeSectionTitle.astro'
import ResumeSectionExperience from '$/components/ResumeSectionExperience.astro'
import ResumeSectionSkills from '$/components/ResumeSectionSkills.astro'
import ResumeSectionEducation from '$/components/ResumeSectionEducation.astro'
import ResumeSectionVolunteering from '$/components/ResumeSectionVolunteering.astro'

import '$/styles/blog.css'

const response = await fetch(
	'https://api.github.com/gists/af71bf672a196bdda6d68cd19ee856ef',
	{
		headers: {
    'Accept': 'application/vnd.github+json'
  }
	}
)
const data = await response.json()
const resumeJSON = data.files['resume.json'].content
const resume = JSON.parse(resumeJSON)

const iconSize = 20

---
<MainLayout>

		<div class="px-8">

			<div class="m-auto max-w-6xl">
				<article class="content">
					<!-- <div> -->
						<header class="mt-10 mb-12 lg:mt-12 lg:mb-14">
							<h1 class="mb-6 font-display leading-snug font-bold text-4xl lg:text-7xl text-zinc-800 dark:text-zinc-100">
								Zane Taylor
							</h1>
							<h2 class="mb-6 text-2xl lg:text-3xl">{resume.basics.label}</h2>
							<div class="mb-10 flex justify-between font-bold">
								<ul>
									<li class="inline-block mr-6"><a href="mailto:{resume.basics.email}"><svg class="inline-block" width={iconSize} height={iconSize} viewBox="0 0 15 15" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M1 2C0.447715 2 0 2.44772 0 3V12C0 12.5523 0.447715 13 1 13H14C14.5523 13 15 12.5523 15 12V3C15 2.44772 14.5523 2 14 2H1ZM1 3L14 3V3.92494C13.9174 3.92486 13.8338 3.94751 13.7589 3.99505L7.5 7.96703L1.24112 3.99505C1.16621 3.94751 1.0826 3.92486 1 3.92494V3ZM1 4.90797V12H14V4.90797L7.74112 8.87995C7.59394 8.97335 7.40606 8.97335 7.25888 8.87995L1 4.90797Z" fill="currentColor" fill-rule="evenodd" clip-rule="evenodd"></path></svg> {resume.basics.email}</a></li>
